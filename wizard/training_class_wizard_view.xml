<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="training_class_wizard_form" model="ir.ui.view">
        <field name="name">training.class.wizard.view.form</field>
        <field name="model">training.class.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"></field>
                        <field name="max_person_wizard"></field>
                    </group>
                </sheet>
                <footer>
                    <button string="Change" name="action_change" type="object" class="oe_highlight"/>
                    <button string="Discard" name="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    

</odoo>
